package com.yili.priority;

class demo extends Thread{
	String  name;
	public demo(){}
	public demo(String name) {
		super();
		this.name = name;
	}
	public void run() {
		super.run();
		System.out.println(getName());
	}
	
}

public class PriorityDemo {

	public static void main(String[] args) {
		
		demo d=new demo();
		d.start();
	}

}
