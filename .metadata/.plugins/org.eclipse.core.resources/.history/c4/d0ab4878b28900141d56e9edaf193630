package com.ArgsDemo;

public class ArgsDemo {

	public static int sum( int...a){
		int r=0;
		for (int i : a) {
			r+=i;
		}
		return r;
		
	}
	
	public static void main(String[] args) {
		
		int a=sum(1,2);
		System.out.println(a);
		
		int b= sum(3,8,6);
		System.out.println(b);
		
		int rusult=sum(12,13,14,15,26);
		System.out.println(rusult);
		
	}

}
