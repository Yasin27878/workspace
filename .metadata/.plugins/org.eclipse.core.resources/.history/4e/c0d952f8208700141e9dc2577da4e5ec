package com.text;

import java.util.Comparator;

public class Comparatorbylengh implements Comparator {

	
	public int compare(Object o1, Object o2) {
		Person p1=(Person)o1;
		Person p2=(Person)o2;
		
		int	res= p1.getName().length()-p2.getName().length();

		return res==0?p1.getName().compareTo(p2.getName()): res ;
	}

}
