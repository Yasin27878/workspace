package com.yili.gongxiang;

class Resource{
	int id;
	String name;
	
}

//产线
class Input implements Runnable{
	
	Resource r;
	int  key =0;
	public Input(Resource r) {
		super();
		this.r = r;
	}

	public void run(){
		while (true) {
			
		
			switch (key++%3) {
			case 0:
				r.id=1111; r.name="iphone6";
				break;
			case 1:
				r.id=2222; r.name="iphone7";	
				break;
			case 2:
				r.id=3333; r.name="iphone8";
				break;

			default:
				break;
			}
		}
	}
}

class Output implements Runnable{
	Resource r;	
	
	public Output(Resource r) {
		super();
		this.r = r;
	}

	public void  run() {
		
	}
	
}

public class Test {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
